{
    "name": "codicastudio/sitemap",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/honeypot",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.8",
        "illuminate/support": "7.0",
        "nesbot/carbon": "^2.0",
        "spatie/crawler": "^4.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^3.0.0",
        "spatie/temporary-directory": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "codicastudiuo\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudiuo\\Sitemap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
